	void quizTwo() {
		int twochoice1;
		int twochoice2;
		int twochoice3;
		int twochoice4;
		int twochoice5;
		int twochoice6;
		int twochoice7;
		int twochoice8;
		int twochoice9;
		int x;
		int twochoice10;
		int twoscore;

		cout << "Pick a number." << endl;
		cout << "How many days a week do you wear black?" << endl;
		cin >> twochoice1;

		cout << "How many LP's do you own?" << endl;
		cout << "(1)0-9, (2) 10-49, (3)50+" << endl;
		cin >> twochoice2;

		cout << "How many pairs of Converse have you gone through?" << endl;
		cout << "(1)0-4, (2)5-9, (3)10+" << endl;
		cin >> twochoice3;

		cout << "How many concerts have you been to this week?" << endl;
		cout << "(1)0-1, (2)2-3, (3) 3+" << endl;
		cin >> twochoice4;

		cout << "One a scale of 0 to 10 how much do you like to mosh?" << endl;
		cout << "(Choose 11 if currently moshing)" << endl;
		cin >> twochoice5;

		cout << "How old were you when you started listening to Jawbreaker?" << endl;
		cout << "(0) 'Whose Jawbreaker?' (1) in the womb (2) 0-12 (3) 13-17 (4) 17+" << endl;
		cin >> twochoice6;

		cout << "How many bands are you in? " << endl;
		cout << "(1)0-1, (2)2-3, (3) 3+" << endl;
		cin >> twochoice7;

		cout << "How many songs less than a minute do you know?" << endl;
		cout << "(1)0-4, (2)5-9, (3)10+" << endl;
		cin >> twochoice8;

		cout << "How many tours have you been on? " << endl;
		cout << "(1)0-1, (2)2-3, (3) 3+" << endl;
		cin >> twochoice9;

		cout << "Pick 1 or 2." << endl;
		cout << "(1)Bernie or (2)Trump?" << endl;
		cin >> x;
		if (x == 1){
			twochoice10 = 4;
		}
		else{
			twochoice10 = -100;
		}

		twoscore = twochoice1 + twochoice2 + twochoice3 + twochoice4 + twochoice5 + twochoice6 + twochoice7 + twochoice8 + twochoice9 + twochoice10;
		if (twoscore >= -94 && twoscore < 15){
			cout << "You can't get on the guest list." << endl;
			cout << "...But in reality it doesn't matter what you like or what you wear. We're all punk at heart. <3 " << endl;
		}
		else if (twoscore >= 15 && twoscore < 30){
			cout << "You might have paid too much for your 60th pressing of I Don't Want to Grow Up but at least you've got some taste. You're on the right path but definitely not with the right shoes. " << endl;
			cout << "...But in reality it doesn't matter what you like or what you wear. We're all punk at heart. <3 " << endl;
		}
		else{
			cout << "Wow. Are you sure you're not Joey Ramone? You are the epitome of punx. " << endl;
			cout << "I bet you saw Black Flag play unreleased demos in you buddy Tommy's basement as his mom made pizza rolls upstairs. " << endl;
			cout << "...But in reality it doesn't matter what you like or what you wear. We're all punk at heart. <3 " << endl;
		}
	}

	void RPG() {
		string name;
